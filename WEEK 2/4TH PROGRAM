#include<iostream>
Using namespace std;
Void swap numbers(int&a,int&b)
{
int temp;
temp=a;
a=b;
b=temp;
}
 Int main(  )
{
Int num1,num2;
Cout<<"enter two numbers:";
Cin>>num1>>num2;
Cout<<"before swapping:"<<end1;
Cout<<"number 1="<<num1<<end1;
Cout<<"number 2="<<num2<<end1;
Swap numbers(num1,num2);
Cout<<"After swapping:"<<end1;
Cout<<"number 1="<<num1<<end1;
Cout<<"number 2="<<num2<<end1;
return 0;
}
